{
    int value = 10;
    boolean isActive = false;
    int result;

    if (isActive > value && value < 20) { 
        print("Condition vérifiée.");
    } else {
        print("Condition non vérifiée.");
    }

    result = value * 2;
    print("Résultat final : ", result);
}
