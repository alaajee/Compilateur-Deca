{
    int a = 10;
    float b = 5.5;
    boolean c = true;
    int result = 0;

    if (a == b && b == c || a == c) { 
        result = result + 1;
    }

    if (b < c) { // Erreur : comparaisons entre `float` et `boolean`
        result = result + 2;
    }

    if (a > c) { // Erreur : comparaisons entre `int` et `boolean`
        result = result + 3;
    }

    if (a == b) { // Erreur : comparaisons entre `int` et `float`
        result = result + 4;
    }

    print(result); // Affiche le résultat
}
