{
    int divisor = 5;
    boolean isActive = false;
    int result;
    result = divisor % isActive;
    print("Résultat du modulo : ", result);
    if (result == 0) {
        print("Modulo réussi.");
    } else {
        print("Modulo non nul.");
    }
}
